    "diffHighlightAdded": {
      "dark": "brightGreen",
      "light": "brightGreen"
    },
    "diffHighlightRemoved": {
      "dark": "brightRed",
      "light": "brightRed"
    },
    "diffAddedBg": {
      "dark": "#1a2e1a",
      "light": "#e6ffe6"
    },
    "diffRemovedBg": {
      "dark": "#2e1a1a",
      "light": "#ffe6e6"
    },
    "diffContextBg": {
      "dark": "gray1",
      "light": "brightWhite"
