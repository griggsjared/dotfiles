    "diffRemovedLineNumberBg": {
      "dark": "#2e1a1a",
      "light": "#ffe6e6"
    },
    "markdownText": {
      "dark": "foreground",
      "light": "black"
    },
    "markdownHeading": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownLink": {
      "dark": "purple",
      "light": "purple"
    },
    "markdownLinkText": {
      "dark": "orange",
      "light": "orange"
    },
    "markdownCode": {
      "dark": "yellow",
      "light": "yellow"
    },
    "markdownBlockQuote": {
      "dark": "brightBlack",
      "light": "gray3"
    },
    "markdownEmph": {
      "dark": "orange",
      "light": "orange"
    },
    "markdownStrong": {
      "dark": "red",
      "light": "red"
    },
    "markdownHorizontalRule": {
      "dark": "gray2",
      "light": "gray2"
    },
    "markdownListItem": {
      "dark": "blue",
      "light": "blue"
    },
    "markdownListEnumeration": {
      "dark": "purple",
      "light": "purple"
    },
    "markdownImage": {
      "dark": "green",
      "light": "green"
    },
    "markdownImageText": {
      "dark": "orange",
      "light": "orange"
    },
    "markdownCodeBlock": {
      "dark": "foreground",
      "light": "black"
    },
    "syntaxComment": {
      "dark": "brightBlack",
      "light": "gray3"
    },
    "syntaxKeyword": {
      "dark": "red",
      "light": "red"
    },
    "syntaxFunction": {
      "dark": "green",
      "light": "green"
    },
    "syntaxVariable": {
      "dark": "foreground",
      "light": "black"
    },
    "syntaxString": {
      "dark": "yellow",
      "light": "yellow"
    },
    "syntaxNumber": {
      "dark": "purple",
      "light": "purple"
    },
    "syntaxType": {
      "dark": "blue",
      "light": "blue"
    },
    "syntaxOperator": {
      "dark": "red",
      "light": "red"
    },
    "syntaxPunctuation": {
      "dark": "foreground",
      "light": "black"
    }
