{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "background": "#171717",
    "foreground": "#fcfcfa",
    "black": "#1a1a1a",
    "red": "#f7768e",
    "green": "#9ece6a",
    "yellow": "#e0af68",
    "orange": "#ff9e64",
    "purple": "#bb9af7",
    "blue": "#7aa2f7",
    "white": "#fcfcfa",
    "brightBlack": "#727072",
    "brightRed": "#f78399",
    "brightGreen": "#a7d278",
    "brightYellow": "#e3b777",
    "brightOrange": "#ffa773",
    "brightPurple": "#c1a4f7",
    "brightBlue": "#87abf7",
    "brightWhite": "#fcfcfa",
    "gray1": "#2c2c2c",
    "gray2": "#3e3e3e",
    "gray3": "#505050"
  },
  "theme": {
    "primary": {
      "dark": "blue",
      "light": "blue"
    },
    "secondary": {
      "dark": "purple",
      "light": "purple"
    },
    "accent": {
      "dark": "orange",
      "light": "orange"
    },
    "error": {
      "dark": "red",
      "light": "red"
    },
    "warning": {
      "dark": "yellow",
      "light": "yellow"
    },
    "success": {
      "dark": "green",
      "light": "green"
    },
    "info": {
      "dark": "blue",
      "light": "blue"
    },
    "text": {
      "dark": "foreground",
      "light": "black"
    },
    "textMuted": {
      "dark": "brightBlack",
      "light": "gray3"
    },
    "background": {
      "dark": "background",
      "light": "white"
    },
    "backgroundPanel": {
      "dark": "gray1",
      "light": "brightWhite"
    },
    "backgroundElement": {
      "dark": "gray2",
      "light": "white"
    },
    "border": {
      "dark": "gray2",
      "light": "gray2"
    },
    "borderActive": {
      "dark": "blue",
      "light": "blue"
    },
    "borderSubtle": {
      "dark": "gray1",
      "light": "gray1"
